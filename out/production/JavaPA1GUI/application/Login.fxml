<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller">
   <children>
      <Button fx:id="loginButton" layoutX="117.0" layoutY="248.0" mnemonicParsing="false" onAction="#login" text="Log In">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <TextField fx:id="usernameTextField" layoutX="50.0" layoutY="87.0" promptText="Username">
         <font>
            <Font size="16.0" />
         </font>
      </TextField>
      <PasswordField fx:id="passwordTextField" layoutX="50.0" layoutY="150.0" promptText="Password">
         <font>
            <Font size="16.0" />
         </font>
      </PasswordField>
      <Label fx:id="loginLabel" layoutX="56.0" layoutY="42.0" text="Department Store Browser">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label fx:id="loginConfirmLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="115.0" layoutY="207.0" textFill="RED" />
   </children>
</AnchorPane>
